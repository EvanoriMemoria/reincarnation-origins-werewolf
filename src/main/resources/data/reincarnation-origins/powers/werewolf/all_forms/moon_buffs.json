{
  "type": "origins:multiple",
  "scoreboard": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "scoreboard objectives add MoonCycle dummy"
        },
        {
          "type": "origins:execute_command",
          "command": "scoreboard players set operator MoonCycle 8"
        }
      ]
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scoreboard objectives remove MoonCycle"
    }
  },
  "save-day": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "execute store result score MoonCycle MoonCycle run time query day"
        },
        {
          "type": "origins:execute_command",
          "command": "scoreboard players operation MoonCycle MoonCycle %= operator MoonCycle"
        }
      ]
    }
  },
  "calculate-moon": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "scoreboard players operation MoonCycle MoonCycle %= operator MoonCycle"
    }
  },
  "full-moon-condition": {
    "type": "origins:action_over_time",
    "interval": 100,
    "entity_action": {
      "type": "origins:apply_effect",
      "effects": [
        {
          "effect": "minecraft:speed",
          "duration": 101,
          "amplifier": 1,
          "is_ambient": true,
          "show_particles": false,
          "show_icon": true
        },
        {
          "effect": "minecraft:haste",
          "duration": 101,
          "amplifier": 1,
          "is_ambient": true,
          "show_particles": false,
          "show_icon": true
        },
        {
          "effect": "minecraft:strength",
          "duration": 101,
          "amplifier": 0,
          "is_ambient": true,
          "show_particles": false,
          "show_icon": true
        },
        {
          "effect": "minecraft:regeneration",
          "duration": 101,
          "amplifier": 0,
          "is_ambient": true,
          "show_particles": false,
          "show_icon": true
        }
      ]
    },
    "condition": {
      "type": "origins:command",
      "command": "execute if score MoonCycle MoonCycle matches 0",
      "comparison": "==",
      "compare_to": 1
    }
  },
